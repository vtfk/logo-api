{
  "version": 2,
  "name": "logo-api",
  "scope": "vtfk",
  "alias": [
    "logo.api.vtfk.no"
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    { "src": "lib/data/epost/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/epost/(.*)", "headers": { "cache-control": "max-age=1209600, public" }, "dest": "lib/data/epost/$1" },
    { "src": "/.*", "status": 308, "headers": { "Location": "https://vtfylke.no/" } }
  ]
}